{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "outputs": {},
  "parameters": {},
  "resources": [
    {
      "apiVersion": "2016-08-01",
      "dependsOn": [
        "OnlineInformationalCards-farm",
        "OnlineInformationalCards-ai"
      ],
      "kind": "app",
      "location": "germanywestcentral",
      "name": "OnlineInformationalCards",
      "properties": {
        "httpsOnly": true,
        "serverFarmId": "OnlineInformationalCards-farm",
        "siteConfig": {
          "alwaysOn": false,
          "appSettings": [
            {
              "name": "APPINSIGHTS_INSTRUMENTATIONKEY",
              "value": "[reference('Microsoft.Insights/components/OnlineInformationalCards-ai').InstrumentationKey]"
            },
            {
              "name": "APPINSIGHTS_PROFILERFEATURE_VERSION",
              "value": "1.0.0"
            },
            {
              "name": "APPINSIGHTS_SNAPSHOTFEATURE_VERSION",
              "value": "1.0.0"
            },
            {
              "name": "ApplicationInsightsAgent_EXTENSION_VERSION",
              "value": "~2"
            },
            {
              "name": "DiagnosticServices_EXTENSION_VERSION",
              "value": "~3"
            },
            {
              "name": "InstrumentationEngine_EXTENSION_VERSION",
              "value": "~1"
            },
            {
              "name": "SnapshotDebugger_EXTENSION_VERSION",
              "value": "~1"
            },
            {
              "name": "XDT_MicrosoftApplicationInsights_BaseExtensions",
              "value": "~1"
            },
            {
              "name": "XDT_MicrosoftApplicationInsights_Mode",
              "value": "recommended"
            }
          ],
          "metadata": [
            {
              "name": "CURRENT_STACK",
              "value": "dotnetcore"
            }
          ]
        }
      },
      "tags": {},
      "type": "Microsoft.Web/sites"
    },
    {
      "apiVersion": "2014-04-01",
      "kind": "web",
      "location": "germanywestcentral",
      "name": "OnlineInformationalCards-ai",
      "properties": {
        "ApplicationId": "OnlineInformationalCards",
        "Application_Type": "web",
        "name": "OnlineInformationalCards-ai"
      },
      "tags": {
        "[concat('hidden-link:', resourceGroup().id, '/providers/Microsoft.Web/sites/', 'OnlineInformationalCards')]": "Resource",
        "displayName": "AppInsightsComponent"
      },
      "type": "Microsoft.Insights/components"
    },
    {
      "apiVersion": "2018-02-01",
      "location": "germanywestcentral",
      "name": "OnlineInformationalCards-farm",
      "properties": {
        "name": "OnlineInformationalCards-farm",
        "perSiteScaling": false,
        "reserved": false
      },
      "sku": {
        "capacity": 1,
        "name": "F1",
        "size": "0",
        "tier": "Free"
      },
      "tags": {},
      "type": "Microsoft.Web/serverfarms"
    }
